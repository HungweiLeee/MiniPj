<% #byebug %>

<% if @photos %>
	<div class="row">
		<% @photos.each do |p| %>
			<div class="col-md-4">
				<div class="panel-default">
					
					<div class="panel-heading preview">
						<%= image_tag p.image.url %>
					</div>
					
					<div class="panel-body">
						<div class="pull-right">
							<i class="fa fa-times fa-lg"></i>
						</div>
					</div>
				</div>
			</div>
		<% end %>
	</div>
<% end %>